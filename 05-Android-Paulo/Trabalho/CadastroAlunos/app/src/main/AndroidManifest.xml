<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.cadastroalunos">

    <application
        android:name="com.orm.SugarApp"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.CadastroAlunos">
        <activity
            android:name=".activitys.professor.ListaProfessorActivity"
            android:exported="false"
            android:label="@string/appBarListProfessorActivity" />
        <activity
            android:name=".activitys.professor.CadastroProfessorActivity"
            android:exported="false"
            android:label="@string/appBarCadastroProfessorActivity" />
        <activity
            android:name=".activitys.curso.CadastroCursoActivity"
            android:exported="false"
            android:label="@string/appBarCadastroCursoActivity" />
        <activity
            android:name=".activitys.curso.ListaCursoActivity"
            android:exported="false"
            android:label="@string/appBarListCursosActivity" />
        <activity
            android:name=".activitys.aluno.CadastroAlunoActivity"
            android:exported="false"
            android:label="@string/appBarCadastroAlunoActivity" />
        <activity
            android:name=".activitys.aluno.ListaAlunoActivity"
            android:exported="false"
            android:label="@string/appBarListAlunosActivity" />
        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:label="@string/appBarMainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="QUERY_LOG"
            android:value="true" />
        <meta-data
            android:name="DOMAIN_PACKAGE_NAME"
            android:value="com.example.cadastroalunos.model" />
        <meta-data
            android:name="DATABASE"
            android:value="cadastro_aluno.db" />
        <meta-data
            android:name="VERSION"
            android:value="1" />
    </application>

</manifest>